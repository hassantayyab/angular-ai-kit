{
  "$schema": "http://json-schema.org/schema",
  "$id": "SchematicsAngularAIKit",
  "title": "Angular AI Kit ng-add schema",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The project to add Angular AI Kit to",
      "$default": {
        "$source": "projectName"
      }
    },
    "skipInstall": {
      "type": "boolean",
      "default": false,
      "description": "Skip installing dependencies"
    }
  }
}
